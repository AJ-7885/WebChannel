{
  "name": "webchanneldev",
  "version": "1.6.0",
  "private": true,
  "homepage": "https://webchannel.dev/",
  "license": "UNLICENSED",
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0"
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.12.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-config-standard": "15.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-standard": "4.0.2",
    "husky": "4.3.0",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2"
  },
  "scripts": {
    "postversion": "yarn publish --tag=next",
    "postpublish": "git push origin --all; git push origin --tags",
    "preversion": "yarn test",
    "start": "react-scripts start",
    "start:https": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/**/*.{js,jsx} --collectCoverageFrom=!src/**/*.{data,story,style}.js",
    "lint": "eslint --cache --fix src/ && prettier 'src/**/*.{js,jsx,json}' --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">12.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --cache --fix",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "keywords": [],
  "description": ""
}
